{
   "Version":"2012-10-17",
   "Id":"PutObjPolicy",
   "Statement":[{
         "Sid":"DenyUnEncryptedObjectUploads",
         "Effect":"Deny",
         "Principal":{
           "AWS" : [
               "arn:aws:iam::{{aws_account}}:role/{{tagprefix}}{{vault_name}}"
           ]
	 },
         "Action":"s3:PutObject",
         "Resource":"arn:aws:s3:::deployutils/*",
         "Condition":{
            "StringNotEquals":{
               "s3:x-amz-server-side-encryption":"AES256"
            }
         }
      }
   ]
}
